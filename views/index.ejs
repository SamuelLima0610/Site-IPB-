<%- include ('./partials/header.ejs') %>
<%- include('./partials/navbar.ejs') %>    
    <!-- ##### Hero Area Start ##### -->
    <section class="hero-area hero-post-slides owl-carousel">
        <!-- Single Hero Slide -->
        <div class="single-hero-slide bg-img bg-overlay d-flex align-items-center justify-content-center" style="background-image: url(/img/bg-img/igreja.png);">
            <!-- Post Content -->
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="hero-slides-content">
                            <h2 data-animation="fadeInUp" data-delay="100ms">Tornando Jesus Conhecido</h2>
                            <p data-animation="fadeInUp" data-delay="300ms">Conheça mais sobre nossa Igreja Presbiteriana Betel</p>
                            <a href="/sobre" class="btn crose-btn" data-animation="fadeInUp" data-delay="500ms">Sobre</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Single Hero Slide -->
        <div class="single-hero-slide bg-img bg-overlay d-flex align-items-center justify-content-center" style="background-image: url(/img/bg-img/bible.jpg);">
            <!-- Post Content -->
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="hero-slides-content">
                            <h2 data-animation="fadeInUp" data-delay="100ms">Uma igreja reformada sempre se reformando pelas Escrituras</h2>
                            <p data-animation="fadeInUp" data-delay="300ms">Conheça mais sobre a fé reformada.</p>
                            <a href="/sobre" class="btn crose-btn" data-animation="fadeInUp" data-delay="500ms">Sobre</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Single Hero Slide -->
        <div class="single-hero-slide bg-img bg-overlay d-flex align-items-center justify-content-center" style="background-image: url(/img/bg-img/workship.jpg);">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="hero-slides-content">
                            <h2 data-animation="fadeInUp" data-delay="100ms">Pregando Cristo, com a graça de Cristo, para a glória de Cristo</h2>
                            <p data-animation="fadeInUp" data-delay="300ms">Conheça nossas crenças ou em quem nós cremos</p>
                            <a href="/sobre" class="btn crose-btn" data-animation="fadeInUp" data-delay="500ms">Sobre</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ##### Hero Area End ##### -->

    <!-- ##### About Area Start ##### -->
    <section class="about-area section-padding-100-0" >
        <div class="container">
            <div class="row">
                <!-- Section Heading -->
                <div class="col-12">
                    <div class="section-heading">
                        <h2>Bem-vindo a nossa igreja</h2>
                        <p>Conheça mais sobre nós.</p>
                    </div>
                </div>
            </div>

            <div class="row about-content justify-content-center">
                <!-- Single About Us Content -->
                <div class="col-12 col-md-6 col-lg-4">
                    <div class="about-us-content mb-100">
                        <img src="/img/bg-img/igreja.png" alt="">
                        <div class="about-text">
                            <h4>Nossa Igreja</h4>
                            <p>Conheça mais sobre a história da Igreja Presbiteriana Betel em Feira de Santana</p>
                            <a href="/sobre">Leia mais<i class="fa fa-angle-double-right"></i></a>
                        </div>
                    </div>
                </div>

                <!-- Single About Us Content -->
                <div class="col-12 col-md-6 col-lg-4">
                    <div class="about-us-content mb-100">
                        <img src="/img/bg-img/history.jpg" alt="">
                        <div class="about-text">
                            <h4>Nossa História</h4>
                            <p>Conheça mais sobre a história da Igreja Presbiteriana no Brasil</p>
                            <a href="/sobre">Leia mais<i class="fa fa-angle-double-right"></i></a>
                        </div>
                    </div>
                </div>

                <!-- Single About Us Content -->
                <div class="col-12 col-md-6 col-lg-4">
                    <div class="about-us-content mb-100">
                        <img src="/img/bg-img/bible.jpg" alt="">
                        <div class="about-text">
                            <h4>Nossos Sermões</h4>
                            <p>Escute e acompanhe nossos sermões em áudio ou pelo YouTube, no canal da nossa igreja.</p>
                            <a href="#sermoes">Leia mais<i class="fa fa-angle-double-right"></i></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ##### About Area End ##### -->

    <!-- ##### Call To Action Area Start ##### -->
    <section class="call-to-action-area section-padding-100 bg-img bg-overlay" style="background-image: url(/img/bg-img/prayer.jpg)">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="call-to-action-content text-center">
                        <h6>Um lugar para você</h6>
                        <h2>Em nossa igreja você encontra um lugar para aprender a Palavra de Deus.</h2>
                        <!--<a href="#" class="btn crose-btn btn-2">Become A Member</a> -->
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ##### Call To Action Area End ##### -->

    <!-- ##### Latest Sermons Area Start ##### -->
    <section class="latest-sermons-area section-padding-100-0" id="sermoes">
        <div class="container">
            <div class="row">
                <!-- Section Heading -->
                <div class="col-12">
                    <div class="section-heading">
                        <h2>Ultimos Sermões</h2>
                        <p>"Prega a palavra, insta, quer seja oportuno, quer não, corrige, repreende, exorta, com toda a longamidade e doutrina."(2 Tm 4:2)</p>
                    </div>
                </div>
            </div>
            <div class="row justify-content-center">
                <% sermons.forEach(sermon => { %>
                    <!-- Single Latest Sermons -->
                    <div class="col-12 col-md-6 col-lg-4">
                        <div class="single-latest-sermons mb-100">
                            <div class="sermons-thumbnail">
                                <img src="/img/bg-img/bible.jpg" alt="">
                                <!-- Date -->
                                <div class="sermons-date">
                                    <h6><span><%= sermon.createdAt.getDate()%></span><%= months[sermon.createdAt.getMonth()]%></h6>
                                </div>
                            </div>
                            <div class="sermons-content">
                                <div class="sermons-cata">
                                    <a href="#" data-toggle="tooltip" data-placement="top" title="Video"><i class="fa fa-video-camera" aria-hidden="true"></i></a>
                                    <a href="#" data-toggle="tooltip" data-placement="top" title="Audio"><i class="fa fa-headphones" aria-hidden="true"></i></a>
                                    <a href="#" data-toggle="tooltip" data-placement="top" title="Docs"><i class="fa fa-file" aria-hidden="true"></i></a>
                                    <a href="#" data-toggle="tooltip" data-placement="top" title="Download"><i class="fa fa-cloud-download" aria-hidden="true"></i></a>
                                </div>
                                <a href="/sermao/<%= sermon.id%>"><h4><%= sermon.title%></h4></a>
                                <div class="sermons-meta-data">
                                    <p><i class="fa fa-user" aria-hidden="true"></i> Sermon From: <span><%= sermon.preacher%></span></p>
                                    <!-- <p><i class="fa fa-tag" aria-hidden="true"></i> Categories: <span>God, Pray</span></p> -->
                                    <!--<p><i class="fa fa-clock-o" aria-hidden="true"></i> <span><%= sermon.createdAt %></span></p> -->
                                </div>
                            </div>
                        </div>
                    </div>
                <% }) %>
            </div>
        </div>
    </section>
    <!-- ##### Latest Sermons Area End ##### -->

    <!-- ##### Upcoming Events Area Start ##### -->
    <section class="upcoming-events-area section-padding-0-100">
        <!-- Upcoming Events Heading Area -->
        <div class="upcoming-events-heading bg-img bg-overlay bg-fixed" style="background-image: url(/img/bg-img/schedule.jpg);">
            <div class="container">
                <div class="row">
                    <!-- Section Heading -->
                    <div class="col-12">
                        <div class="section-heading text-left white">
                            <h2>Eventos</h2>
                            <p>Eventos públicos da igreja</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Upcoming Events Slide -->
        <div class="upcoming-events-slides-area">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="upcoming-slides owl-carousel">
                            <div class="single-slide">
                                <% notices.forEach((notice,i) => { %>
                                    <%if(i < 3){%>
                                        <!-- Single Upcoming Events Area -->
                                        <div class="single-upcoming-events-area d-flex flex-wrap align-items-center">
                                            <!-- Thumbnail -->
                                            <div class="upcoming-events-thumbnail">
                                                <img src="<%= notice.image%>" alt="">
                                            </div>
                                            <!-- Content -->
                                            <div class="upcoming-events-content d-flex flex-wrap align-items-center">
                                                <div class="events-text">
                                                    <a href="/evento/<%= notice.id%>"><h4><%= notice.title%></h4></a>
                                                    <div class="events-meta">
                                                        <a href="#"><i class="fa fa-calendar" aria-hidden="true"></i> <%= notice.date.getDate()%>/<%= months[notice.date.getMonth()]%>/<%= notice.date.getFullYear()%></a>
                                                        <a href="#"><i class="fa fa-clock-o" aria-hidden="true"></i> <%= notice.time %></a>
                                                        <!--<a href="#"><i class="fa fa-map-marker" aria-hidden="true"></i> 11 Rose St, Brooklyn, NY</a>-->
                                                    </div>
                                                    <!-- <p><%- notice.notice%></p> -->
                                                    <a href="#">Read More <i class="fa fa-angle-double-right"></i></a>
                                                </div>
                                                <div class="find-out-more-btn">
                                                    <a href="/evento/<%= notice.id%>" class="btn crose-btn btn-2">Veja!</a>
                                                </div>
                                            </div>
                                        </div>
                                    <%}%>
                                <%})%>
                            </div>    
                            <%if(notices.length > 3){%>
                                <div class="single-slide">
                                    <% notices.forEach((notice,i) => { %>
                                        <%if(i >= 3){%>
                                            <!-- Single Upcoming Events Area -->
                                            <div class="single-upcoming-events-area d-flex flex-wrap align-items-center">
                                                <!-- Thumbnail -->
                                                <div class="upcoming-events-thumbnail">
                                                    <img src="<%= notice.image%>" alt="">
                                                </div>
                                                <!-- Content -->
                                                <div class="upcoming-events-content d-flex flex-wrap align-items-center">
                                                    <div class="events-text">
                                                        <a href="/evento/<%= notice.id%>"><h4><%= notice.title%></h4></a>
                                                        <div class="events-meta">
                                                            <a href="#"><i class="fa fa-calendar" aria-hidden="true"></i> <%= notice.date.getDate()%>/<%= months[notice.date.getMonth()]%>/<%= notice.date.getFullYear()%></a>
                                                            <a href="#"><i class="fa fa-clock-o" aria-hidden="true"></i> <%= notice.time %></a>
                                                            <!--<a href="#"><i class="fa fa-map-marker" aria-hidden="true"></i> 11 Rose St, Brooklyn, NY</a>-->
                                                        </div>
                                                        <!-- <p><%- notice.notice%></p> -->
                                                        <a href="#">Read More <i class="fa fa-angle-double-right"></i></a>
                                                    </div>
                                                    <div class="find-out-more-btn">
                                                        <a href="#" class="btn crose-btn btn-2">Find Out More</a>
                                                    </div>
                                                </div>
                                            </div>
                                        <%}%>
                                    <%})%>
                                </div>
                            <%}%>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ##### Upcoming Events Area End ##### -->

